# Пример файла с переменными окружения для проекта

# Токен Telegram-бота
BOT_TOKEN=

# Имя базы данных
DB_NAME=

# Пользователь базы данных
DB_USER=

# Пароль пользователя базы данных
DB_PASSWORD=

# Хост сервера базы данных (опционально, по умолчанию localhost)
DB_HOST=

# Порт сервера базы данных (опционально, по умолчанию 5432)
DB_PORT=

# Секрет для проверки веб-хуков
WEBHOOK_SECRET=

# Доверять ли заголовку X-Forwarded-For.
# Включайте только при работе за надёжным прокси.
TRUST_FORWARDED=false

